
public class MyDate {
	
	private int dd;
	private int mm;
	private int yy;
	
	
	public MyDate() {
		System.out.println("defafult constructor called");
		dd =1;
		mm = 1;
		yy = 2024;
	}
	
	public MyDate(int d, int m, int y) {
		System.out.println("Patameterized constructor called");
		dd = d;
		mm = m;
		yy = y;
	}
	
	public void display() {
		System.out.println(dd+"/"+mm+"/"+yy);
	}
	

}
