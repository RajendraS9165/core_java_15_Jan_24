
public class MyDate {
	
	private int dd;
	private int mm;
	private int yy;
	
	
	public MyDate() {
		System.out.println("defafult constructor called");
		dd =1;
		mm = 1;
		yy = 2024;
	}
	
	public MyDate(int dd, int mm, int y) {
		System.out.println("Patameterized constructor called");
		dd = dd;
		mm = mm;
		yy = y;
	}
	
	public void display() {
		System.out.println(dd+"/"+mm+"/"+yy);
	}
	
	public void setDay(int d){
		dd = d;
	}
	
	public void setMm(int m) {
		mm = m;
	}
	
	public int getYear() {
		return yy;
	}
	
	
	

}
