package com;

public class LastDay {
	
	
	int dd = 10;
	int mm = 01;
	int yy = 2024;

	public boolean isLeapYear(int y) {
		if((y%4==0 & y%400==0) | (y%100!=0 & y%4==0)) {
//			System.out.println("Leap Year");
			return true;
		}else {
//			System.out.println("Normal year");
			return false;
		}
	}

}
